(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: sirius   description-file: {File :context ".package"}
    (<Project-Directory>                  directory: {Directory :context "src"}
      (<Project-Directory>                directory: {Directory :context "src" "sirius"}
        (<Jazz-File>       name: settings source: {File :context "src" "sirius" "settings.jazz"})
        (<Jazz-File>       name: product  source: {File :context "src" "sirius" "product.jazz"}))))))
