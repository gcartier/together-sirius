(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: sirius  description-file: {File :context ".package"}
    (<*>                                 tag-reference: {File :context "sirius.project"})
    (<Project-Directory>                 directory: {Directory :context "src"}
      (<Project-Directory>               directory: {Directory :context "src" "sirius"}
        (<Jazz-File>       name: appl    source: {File :context "src" "sirius" "appl.jazz"})
        (<Jazz-File>       name: product source: {File :context "src" "sirius" "product.jazz"})
        (<Jazz-File>       name: run     source: {File :context "src" "sirius" "run.jazz"}))))))
