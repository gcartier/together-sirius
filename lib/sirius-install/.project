(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: sirius-install description-file: {File :context ".package"}
    (<*>                                        tag-reference: {File :context "sirius-install.project"})
    (<Project-Directory>                        directory: {Directory :context "src"}
      (<Project-Directory>                      directory: {Directory :context "src" "sirius-install"}
        (<Jazz-File>       name: run            source: {File :context "src" "sirius-install" "run.jazz"})
        (<Jazz-File>       name: appl           source: {File :context "src" "sirius-install" "appl.jazz"}))))))
